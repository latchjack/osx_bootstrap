alias infra_up="docker-compose -f /Users/tobyhamand/Work/TripTease/dungeon-master/runbook/local/dev-infrastructure/docker-compose.yml up &"
alias infra_down="docker-compose -f /Users/tobyhamand/Work/TripTease/dungeon-master/runbook/local/dev-infrastructure/docker-compose.yml down &"
alias infra_setup_mongo="/Users/tobyhamand/Work/TripTease/dungeon-master/runbook/local/copy_mongo_to_local.sh"
alias gpa="ORIG_DIR=pwd; cd /Users/tobyhamand/Work/TripTease/dungeon-master/runbook/local/../../..; find . -type d -depth 1 -exec git --git-dir={}/.git --work-tree=/Users/tobyhamand/Work/TripTease/dungeon-master/runbook/local/{} pull origin master \; cd "
alias l="ls -lah"
alias ni="yarn install"
alias nt="yarn test"
alias ns="yarn start"
alias nr="yarn run"
alias gi="git"
alias o="open ."
alias br-del="git branch --merged | grep -v 'master' | grep -v \* | xargs git branch -D"
alias gdalbs="git fetch -p && git branch -vv | awk '/: gone]/{print $1}'"
alias gdalb="gdalbs | xargs git branch -d"
alias gdalbf="gdalbs | xargs git branch -D"
alias gdarbs="git fetch origin && git branch --remote --merged | grep origin | grep -v '>' | grep -v master | grep -v develop | xargs -L1 | cut -d'/' -f2-"
alias gdarb="gdarbs | xargs git push origin --delete"
alias gdabs="echo "REMOTE:" && gdarbs && echo "" && echo "LOCAL:" && gdalbs"
alias gdab="gdarb || gdalb"
alias gdabf="gdarb || gdalbf"
alias gu="gco master && gpls && gdab"
function pull_all() { for projectPath in $(pwd) ; do cd $projectPath; git reset --hard; git checkout .; gco master; git pull; cd ..; done }
